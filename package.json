{
  "name": "csb-node",
  "version": "1.0.0",
  "description": "CSB Node|Browser Package to Send Events",
  "main": "dist/node/csb.min.js",
  "author": "Amritpal Singh <amrit@customersuccessbox.com>",
  "license": "MIT",
  "scripts": {
    "build": "cross-env OUTPUT_TARGET=web webpack --config config/webpack.prod.js && cross-env OUTPUT_TARGET=web NODE_ENV=production webpack --config config/webpack.prod.js",
    "build:node": "cross-env OUTPUT_TARGET=node webpack --config config/webpack.prod.js && cross-env OUTPUT_TARGET=node NODE_ENV=production webpack --config config/webpack.prod.js",
    "test": "cross-env ENDPOINT= API_KEY= jest --config jest.config.js --detectOpenHandles"
  },
  "keywords": [
    "csb",
    "csb-node",
    "csb-events",
    "csb-browser"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@jest/globals": "^25.5.0",
    "axios": "^0.19.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.4",
    "jest": "^25.5.0",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
